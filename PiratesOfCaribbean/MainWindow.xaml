<Window x:Class="PiratesOfCaribbean.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PiratesOfCaribbean"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="61,55,0,0" VerticalAlignment="Top" Click="button_Click"/>
        <Button x:Name="btnBonus" Content="Bonus" HorizontalAlignment="Left" Margin="389,75,0,0" VerticalAlignment="Top" Click="btnBonus_Click"/>
        <ListBox x:Name="lstNames" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="517,75,0,0"/>

    </Grid>
</Window>
